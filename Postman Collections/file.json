  "item": [
    {
      "name": "first request ever",
      "event": [
        {
          "listen": "prerequest",
          "script": {
            "id": "a94c7f22-4615-4583-8eed-cec77fa91477",
            "exec": [
              "var previousRequest = JSON.parse(globals.previousRequest),",
              "    webhookRequestData = previousRequest.data;",
              "",
              "// webhookRequestData contains the data sent to your webhook.",
              "console.log(webhookRequestData);",
              "pm.variables.set('response_url', webhookRequestData.response_url)",
              ""
            ],
            "type": "text/javascript"
          }
        }
      ],
      "_postman_id": "e2a19463-f9b1-4e64-b3fa-38639a4c6953",
      "protocolProfileBehavior": {
        "disableBodyPruning": true
      },
      "request": {
        "method": "POST",
        "header": [],
        "body": {
          "mode": "raw",
          "raw": "{\n    \"text\": \"Hello\"\n}",
          "options": {
            "raw": {
              "language": "json"
            }
          }
        },
        "url": "{{response_url}}"
      },
      "response": []
    },
